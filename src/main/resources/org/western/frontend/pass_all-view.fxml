<?xml version="1.0" encoding="UTF-8"?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.image.ImageView?>

<?import java.net.URL?>
<AnchorPane xmlns:fx="http://javafx.com/fxml" fx:controller="org.western.frontend.PassAllController" styleClass="background" prefHeight="500.0" prefWidth="600.0">
    <stylesheets>
        <URL value="@style.css" />
    </stylesheets>
    <children>
        <VBox alignment="CENTER" AnchorPane.topAnchor="1.0" AnchorPane.rightAnchor="20.0" AnchorPane.bottomAnchor="20.0" AnchorPane.leftAnchor="20.0">
            <ImageView fitWidth="540.0" fitHeight="380.0" preserveRatio="true" fx:id="imageView">
            </ImageView>
        </VBox>
        <Button text="Main Menu >" AnchorPane.rightAnchor="20.0" AnchorPane.bottomAnchor="20.0" fx:id="mainMenuButton" onAction="#handleMainMenuAction"/>
    </children>
</AnchorPane>
