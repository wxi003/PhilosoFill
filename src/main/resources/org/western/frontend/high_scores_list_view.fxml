<?xml version="1.0" encoding="UTF-8"?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<?import java.net.URL?>
<AnchorPane id="AnchorPane" styleClass="background" prefHeight="500.0" prefWidth="600.0" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.western.frontend.HighScoresListController">
    <stylesheets>
        <URL value="@style.css" />
    </stylesheets>

    <children>
        <Button fx:id="backButton" layoutX="10" layoutY="10" text="Back" onAction="#handleBackButtonAction"/>
        <VBox alignment="CENTER" layoutX="50.0" layoutY="50.0" prefHeight="400.0" prefWidth="500.0">
            <children>
                <TableView fx:id="scoresTable" prefHeight="300.0" prefWidth="500.0">
                    <columns>
                        <TableColumn fx:id="column1" prefWidth="165" text="RANK"/>
                        <TableColumn fx:id="column2" prefWidth="165" text="SCORE"/>
                        <TableColumn fx:id="column3" prefWidth="165" text="NAME"/>
                    </columns>
                </TableView>
            </children>
        </VBox>
    </children>
</AnchorPane>